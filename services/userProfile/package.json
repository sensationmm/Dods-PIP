{
  "name": "user-profile",
  "version": "1.0.0",
  "description": "User Profile service for Dods' PIP",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "scripts/setUp.sh && cucumber-js && node cucumber.html.reporter.js && scripts/tearDown.sh",
    "test:report": "open coverage/integration-tests-report.html && open coverage/lcov-report/index.html",
    "dev": "scripts/setUpDev.sh && AWS_PROFILE=dods-dev AWS_REGION=eu-west-1 NODE_ENV=dev serverless offline --stage dev && scripts/tearDown.sh",
    "validate:env": "SERVERLESS_STAGE=prod ts-node ./src/domain/config/index.ts",
    "package": "serverless package",
    "deploy:dev": "AWS_PROFILE=dods-dev AWS_REGION=eu-west-1 serverless deploy --stage dev",
    "deploy:prod": "AWS_REGION=eu-west-1 serverless deploy --stage prod",
    "remove:deploy:dev": "AWS_PROFILE=dods-dev AWS_REGION=eu-west-1 serverless remove --stage dev",
    "remove:deploy:prod": "AWS_REGION=eu-west-1 serverless remove --stage prod",
    "serverlessConfig": "serverless config credentials --provider aws --profile dods --key {aws_access_key_id} --secret {aws_secret_access_key}"
  },
  "pre-commit": [
    "test:coverage",
    "test:integration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/DodsGroup/dodsmlp/src/master/services/userProfile/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://dods-support.atlassian.net/secure/RapidBoard.jspa?rapidView=19&projectKey=DOD"
  },
  "homepage": "https://bitbucket.org/kenanhancerdod/dods-nodejs-lambda-boilerplate#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.1",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "@cucumber/messages": "17.1.1",
    "@types/aws-lambda": "^8.10.83",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.1",
    "copy-webpack-plugin": "^9.0.1",
    "cucumber-html-reporter": "^5.5.0",
    "jest": "^27.1.0",
    "mockserver-client": "^5.11.2",
    "open": "^8.2.1",
    "grunt": "^1.0.0",
    "pre-commit": "^1.2.2",
    "serverless": "^2.57.0",
    "serverless-dependson-plugin": "^1.1.2",
    "serverless-localstack": "^0.4.34",
    "serverless-offline": "^8.1.0",
    "serverless-prune-plugin": "^1.5.1",
    "serverless-webpack": "^5.5.4",
    "inquirer": "8.1.5",
    "swagger-parser": "^10.0.3",
    "openapi-types": "9.3.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "@types/node": "16.10.1",
    "typescript": "^4.4.2",
    "webpack": "^5.52.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@dodsgroup/dods-lambda": "^1.1.5",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.979.0",
    "axios": "^0.21.1",
    "joi": "^17.4.2"
  }
}
